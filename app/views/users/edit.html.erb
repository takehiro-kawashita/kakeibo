<h1>会員情報更新</h1>
<%= form_with model: @user,url: user_path, method: "patch",local: true do |f| %>

    <div class="form-group">
        <label>名前</label>
        <%= f.text_field :name,class: "form-control"%>
    </div>
    
    <div class="form-group">
        <label>メールアドレス</label>
        <%= f.text_field :email,class: "form-control"%>
    </div>
    
    <%= f.submit "更新",class: "btn btn-primary" %>

<% end %>
